{% extends "_layouts/cp" %}

{% from 'webperf/_includes/macros.twig' import registerJsModules %}

{% block head %}
    {{ parent() }}
    {{ craft.webperf.includeCssModule("vendors.css") }}
    {{ craft.webperf.includeCssModule("styles.css") }}
{% endblock %}

{% block foot %}
    {{ parent() }}
    {% set jsModules = [
        'runtime.js',
        'vendors.js',
        'commons.js',
        'webperf.js',
    ] %}
    {{ registerJsModules(jsModules) }}
{% endblock %}
