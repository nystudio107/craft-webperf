div.webperf-button-container {
  display: inline-block;
  margin-right: 10px;
}

[v-cloak] > * { display:none; }

.webperf-mobile-icon::before {
  content: " ";
  display: inline-block;
  width: 16px;
  height: 16px;
  vertical-align: sub;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}

.webperf-desktop-icon::before {
  content: " ";
  display: inline-block;
  width: 16px;
  height: 16px;
  vertical-align: sub;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}
