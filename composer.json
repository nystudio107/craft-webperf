{
    "name": "nystudio107/craft-webperf",
    "description": "Monitor the performance of your webpages through real-world user timing data",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "webperf"
    ],
    "support": {
        "docs": "https://github.com/nystudio107/craft-webperf/blob/master/README.md",
        "issues": "https://github.com/nystudio107/craft-webperf/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "nystudio107",
            "homepage": "https://nystudio107.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "whichbrowser/parser": "^2.0.37"
    },
    "autoload": {
        "psr-4": {
          "nystudio107\\webperf\\": "src/"
        }
    },
    "extra": {
        "name": "Webperf",
        "handle": "webperf",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/nystudio107/craft-webperf/master/CHANGELOG.md",
        "components": {
            "metrics": "nystudio107\\webperf\\services\\Metrics",
            "beacon": "nystudio107\\webperf\\services\\Beacon"
        },
        "class": "nystudio107\\webperf\\Webperf"
    }
}
